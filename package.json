{
  "name": "typesbcp47",
  "productName": "TypesBCP47",
  "version": "1.16.0",
  "description": "TypeScript library for parsing BCP47 language tags",
  "keywords": [
    "typescript",
    "bcp47",
    "language",
    "tag",
    "locale",
    "i18n",
    "l10n"
  ],
    "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json && node scripts/create-esm-package.cjs"
  },"author": {
    "name": "Rodolfo M. Raya",
    "url": "https://maxprograms.com",
    "email": "rmraya@maxprograms.com"
  },
  "license": "EPL-1.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json",
    "./cjs/*": "./dist/cjs/*"
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmraya/TypesBCP47.git"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "typesxml": "^1.17.0"
  }
}